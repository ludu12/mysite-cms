{
  "name": "mysite-cms",
  "version": "1.0.0",
  "description": "Keystone JS CMS",
  "main": "src/index.js",
  "author": "Luke Dunscombe",
  "license": "ISC",
  "engines": {
    "node": ">=8.4.0",
    "yarn": ">=1.12.3"
  },
  "scripts": {
    "babel": "babel src/ --copy-files --out-dir build --source-maps \\\"inline\\\"",
    "build": "npm run babel && cross-env NODE_ENV=production keystone build",
    "clean": "rimraf ./dist && rimraf ./build",
    "format": "prettier --write **/*.{js,jsx,tsx}",
    "lint": "eslint --fix '{src,server}/**/*.{js,jsx,tsx}'",
    "start": "cross-env NODE_ENV=production keystone start",
    "start:local": "npm run babel && cross-env NODE_ENV=development DISABLE_LOGGING=true keystone dev",
    "start:watch": "nodemon --watch src/ --exec npm run server"
  },
  "dependencies": {
    "@keystonejs/adapter-mongoose": "^5.1.3",
    "@keystonejs/app-admin-ui": "^5.3.0",
    "@keystonejs/app-graphql": "^5.0.1",
    "@keystonejs/app-static": "^5.0.0",
    "@keystonejs/auth-password": "^5.0.1",
    "@keystonejs/fields": "^6.0.0",
    "@keystonejs/fields-wysiwyg-tinymce": "^5.0.3",
    "@keystonejs/file-adapters": "^5.3.0",
    "@keystonejs/keystone": "^5.3.0",
    "babel-plugin-istanbul": "^5.2.0",
    "babel-plugin-styled-components": "^1.10.7",
    "cross-env": "^5.2.0",
    "dotenv": "^8.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/plugin-proposal-optional-chaining": "^7.7.5",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/runtime": "7.4.4",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.13.0",
    "jest": "^24.8.0",
    "prettier": "^1.17.1"
  }
}
